def SearchingChallenge(strParam):

  strParam = strParam.lower()
  results = []
  resp = 'none'
  for i in range(len(strParam)):
    for j in range(0, i):
      chunk = strParam[j:i + 1]

      if chunk == chunk[::-1]:
        results.append(chunk)
        resp = max(results)
  if len(resp) < 3:
    resp = 'none'

  return resp

# keep this function call here 
print(SearchingChallenge(input()))
